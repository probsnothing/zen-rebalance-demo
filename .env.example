# Copy this file to `.env` and fill in your runtime configuration.

# Solana RPC endpoint with your API key if required.
SOLANA_RPC_URL="https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY"

# JSON array version of the fee-payer secret key (keep this encrypted in production).
SOLANA_KEYPAIR_SECRET="[your json array secret key here]"
# Comma-separated list of token mint addresses the bot should balance (currently requires two).
TOKEN_MINTS="token1,token2"

# Optional: override the rebalance threshold percentage (default 1.7%).
REBALANCE_THRESHOLD_PERCENT="1.7"
